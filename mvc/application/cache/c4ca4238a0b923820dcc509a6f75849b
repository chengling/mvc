    <script language = "javascript" type = "text/javascript" >

        function ToogleChildrenAge() {

            var childAgeSelect = document.getElementById("ddlChildAge");
            var rdolist = document.getElementsByName("rblCarryChild");
            if (rdolist[1].checked)
                childAgeSelect.disabled = false;
            if (rdolist[2].checked)
                childAgeSelect.disabled = true;
        }
    	    
    	
    </script> 
	
	
