<script>
  function choose(v){
   if(!v){
     v=$("select[name='txtCountryTo']").val();
   }
  $('#article_id').empty();
    $.post("http://localhost/mvc/index.php/index/ajax",{
				total_id:v,
				vip_password:0
			  },
			  function(data,status){
				 var result = eval ("(" + data + ")");
				     ln=result.body;
				  var array=new Array();
				  for(var i=0,doc; i<ln.length; i++) {
				  
				  
				      doc='<option '; 
					  doc+='value="'+ln[i].id+'">'+ln[i].name+'&nbsp;&nbsp;'+'</option>';
					  $('#article_id').append(doc);
				  }
				 // console.log(array);
				 
			  });
  }
  $(function(){
    choose(0);
  });
</script>

